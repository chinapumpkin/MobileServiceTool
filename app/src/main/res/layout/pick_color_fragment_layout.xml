<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:baselineAligned="false">
    <RelativeLayout android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:orientation="horizontal">
        <LinearLayout android:layout_width="wrap_content"
                      android:layout_height="wrap_content"
                      android:orientation="horizontal">
            <Button android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_margin="2dp"
                    android:tag="1"
                    android:text="1"
                    android:id="@+id/led1"/>
            <Button android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_margin="2dp"
                    android:tag="2"
                    android:text="2"
                    android:id="@+id/led2"/>
            <Button android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_margin="2dp"
                    android:tag="3"
                    android:text="3"
                    android:id="@+id/led3"/>
        </LinearLayout>
        <LinearLayout android:layout_width="wrap_content"
                      android:layout_height="wrap_content"
                      android:orientation="horizontal"
                      android:layout_alignParentRight="true">
            <Button android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_margin="2dp"
                    android:tag="4"
                    android:text="4"
                    android:id="@+id/led4"/>
            <Button android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_margin="2dp"
                    android:tag="5"
                    android:text="5"
                    android:id="@+id/led5"/>
            <Button android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_margin="2dp"
                    android:tag="6"
                    android:text="6"
                    android:id="@+id/led6"/>
        </LinearLayout>
    </RelativeLayout>
    <RelativeLayout android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:orientation="horizontal">
        <LinearLayout android:layout_width="wrap_content"
                      android:layout_height="wrap_content"
                      android:orientation="horizontal">
            <Button android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_margin="2dp"
                    android:tag="14"
                    android:text="14"
                    android:id="@+id/led14"/>
        </LinearLayout>
        <LinearLayout android:layout_width="wrap_content"
                      android:layout_height="wrap_content"
                      android:orientation="horizontal"
                      android:layout_alignParentRight="true">
            <Button android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_margin="2dp"
                    android:layout_alignParentRight="true"
                    android:tag="7"
                    android:text="7"
                    android:id="@+id/led7"/>
        </LinearLayout>
    </RelativeLayout>
    <RelativeLayout android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:orientation="horizontal">
        <LinearLayout android:layout_width="wrap_content"
                      android:layout_height="wrap_content"
                      android:orientation="horizontal">
            <Button android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_margin="2dp"
                    android:tag="13"
                    android:text="13"
                    android:id="@+id/led13"/>
            <Button android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_margin="2dp"
                    android:tag="12"
                    android:text="12"
                    android:id="@+id/led12"/>
            <Button android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_margin="2dp"
                    android:tag="11"
                    android:text="11"
                    android:id="@+id/led11"/>
        </LinearLayout>
        <LinearLayout android:layout_width="wrap_content"
                      android:layout_height="wrap_content"
                      android:orientation="horizontal"
                      android:layout_alignParentRight="true">
            <Button android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_margin="2dp"
                    android:tag="10"
                    android:text="10"
                    android:id="@+id/led10"/>
            <Button android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_margin="2dp"
                    android:tag="9"
                    android:text="9"
                    android:id="@+id/led9"/>
            <Button android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_margin="2dp"
                    android:tag="8"
                    android:text="8"
                    android:id="@+id/led8"/>
        </LinearLayout>

    </RelativeLayout>
    <SeekBar
            android:id="@+id/seekbar_font"
            android:layout_width="300dip"
            android:layout_height="wrap_content"
            android:layout_margin="10px"
            android:max="100"
            android:progress="50"></SeekBar>
    <LinearLayout android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:orientation="horizontal">
        <LinearLayout android:layout_width="wrap_content"
                      android:layout_height="wrap_content"
                      android:orientation="vertical">
            <TextView android:layout_width="wrap_content"
                      android:layout_height="wrap_content"
                      android:text="@string/label_frequency"
                      android:textSize="18sp"/>
            <Space android:layout_width="5dp"
                   android:layout_height="wrap_content"/>
            <EditText android:id="@+id/frequency_text"
                      android:layout_width="match_parent"
                      android:layout_height="wrap_content"
                      android:inputType="numberDecimal"
                      android:textSize="18sp"/>
        </LinearLayout>
        <LinearLayout android:layout_width="wrap_content"
                      android:layout_height="wrap_content"
                      android:orientation="vertical">
            <TextView android:layout_width="wrap_content"
                      android:layout_height="wrap_content"
                      android:text="@string/label_duty_cycle"
                      android:textSize="18sp"/>
            <Space android:layout_width="5dp"
                   android:layout_height="wrap_content"/>
            <EditText android:id="@+id/duty_cycle_text"
                      android:layout_width="match_parent"
                      android:layout_height="wrap_content"
                      android:inputType="numberDecimal"
                      android:textSize="18sp"/>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:orientation="horizontal">
        <Button android:layout_width="90dp"
                android:layout_height="40dp"
                android:layout_margin="2dp"
                android:text="Set"
                android:id="@+id/set_button"/>
        <Button android:layout_width="90dp"
                android:layout_height="45dp"
                android:layout_margin="2dp"
                android:text="Cancel"
                android:id="@+id/cancel_button"/>
    </LinearLayout>
</LinearLayout>